{% extends 'layout.html.twig' %}

{% block title %}Artiste{% endblock %}

{% block body %}
    <h1>Artiste</h1>

    <table class="table">
        <tbody>
        <tr>
            <td>{{ artiste.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ artiste.name }}</td>
        </tr>
        <tr>
            <th>Content</th>
            <td>{{ artiste.content }}</td>
        </tr>
        <tr>
            <th>Image</th>
            <td>{{ artiste.image }}</td>
        </tr>
        <tr>
            <th>Specialite</th>
            <td>{{ artiste.specialite }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('artiste_index') }}">back to list</a>

{% endblock %}
